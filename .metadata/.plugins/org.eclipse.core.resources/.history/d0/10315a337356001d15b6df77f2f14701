package solution1;

public class solution {
	public static int[] reverse(int arr[]) {
		int l = arr.length;
		int[] op = new int[l];
		int temp = l;
		for(int i=0; i<l; i++) {
			op[temp-1] = arr[i];
			temp -= 1;
		}
		
		return op;
	}
	
	public static void main(String[] args)
    {
        int[] arr = {10, 20, 30, 40, 50};
        reverse(arr);
    }
}

