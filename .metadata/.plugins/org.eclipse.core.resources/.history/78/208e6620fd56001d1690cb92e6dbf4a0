package test;

public class test {
	public int[][] transpose(int[][] array)
	{
    	int m = array.length; //the matrix is square
        int[][] op = new int[m][m];
            
        for(int i=0; i<m; i++)
        {
        	for(int j=0; j<m; j++)
            {
        		op[i][j] = array[j][i];
            }
        }
        return op;
    } 

	public static void main(String[] args)
	{
		String mat = "[[2,6,7],[9,5,8],[5,4,3]]";
		String[] splitt = mat.split("2");
		System.out.println(splitt);
	}
}