package test;

public class test {
	public int[][] transpose(int[][] array)
	{
    	int m = array.length; //the matrix is square
        int[][] op = new int[m][m];
            
        for(int i=0; i<m; i++)
        {
        	for(int j=0; j<m; j++)
            {
        		op[i][j] = array[j][i];
            }
        }
        return op;
    } 

	public static void main(String[] args)
	{
		String mat = "[[4,5],[9,8]]";
		mat = mat.replace("],[", "/");
		mat = mat.replace("[[", "");
		mat = mat.replace("]]", "");
		String[] op = mat.split("/");
		int[] res = new int[3];
		for(String i: op) {
			int[i] = op[i];
		}
	}
}